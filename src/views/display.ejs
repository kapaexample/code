<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>INF</title>
    <style>
        .button {
          border: none;
          color: white;
          padding: 16px 32px;
          text-align: center;
          text-decoration: none;
          display: inline-block;
          font-size: 16px;
          margin: 4px 2px;
          transition-duration: 0.4s;
          cursor: pointer;
        }
        
        .button1 {
          background-color: white; 
          color: black; 
          border: 2px solid #4CAF50;
        }
        
        .button1:hover {
          background-color: #4CAF50;
          color: white;
        }
        
        .button2 {
          background-color: white; 
          color: black; 
          border: 2px solid #008CBA;
        }
        
        .button2:hover {
          background-color: #008CBA;
          color: white;
        }
        
        </style>
    <style>
        table, th, td {
          border:1px solid black;
          border-collapse: collapse;
        }
        </style>
</head>
<body>
    <h2>Info User</h2>

<table style="width:100%">
  <tr>
    <th>Email</th>
    <th>Password</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Action</th>
  </tr>
  <tr>
    <% for(var i=0; i < dataTable.length; i++) { %>
        <tr>
          <td><%= dataTable[i].email %></td>
          <td><%= dataTable[i].password %></td>
          <td><%= dataTable[i].firstName %></td>
          <td><%= dataTable[i].lastName %></td>
          <td><button class="button button1">delete</button>
          <button class="button button2">edit</button></td>
          
        </tr>
     <% } %>
  </tr>
</table>
</body>
</html>